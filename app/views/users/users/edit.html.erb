ユーザー編集
<%= form_with model:@user, local:true do |f| %>
<%= f.label :profile_image %>
<%= attachment_image_tag @user, :profile_image, :fill, 60, 60, fallback: "noimage.png" %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :introduction %>
<%= f.text_area :introduction %>
<%= f.label :job_category %>
<%= f.text_field :job_category %>
<%= f.submit %>
<% end %>